package com.example.jwtnew.service;

import java.util.ArrayList;
import java.util.List;
import java.util.UUID;

import org.springframework.stereotype.Service;

import com.example.jwtnew.model.User;

@Service
public class userservice {
	
	
private List<User> store = new ArrayList<>();

public userservice() {

	store.add(new User(UUID.randomUUID().toString(),"vinit@gmail.com","vinit","vinit"));
	store.add(new User(UUID.randomUUID().toString(),"raj@gmail.com","raj","raj1"));
	store.add(new User(UUID.randomUUID().toString(),"jsr@gmail.com","jsr","jsr"));
	store.add(new User(UUID.randomUUID().toString(),"jr@gmail.com","jr","jr"));
	
	
	
	
}


public List<User> getusers()
{
	return store;
}
	




}
